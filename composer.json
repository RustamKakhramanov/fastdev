{
    "name": "kraify/fastdev",
    "description": "Traits and helpers for based functonality",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Kraify\\Fastdev\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rustam Kakhramanov"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "doctrine/dbal": "^@dev",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "@dev",
        "laravel/passport": "@dev",
        "laravel/sanctum": "@dev",
        "laravel/telescope": "@dev",
        "laravel/tinker": "@dev",
        "laravel/ui": "@dev",
        "spatie/laravel-fractal": "@dev",
        "spatie/laravel-permission": "@dev",
        "staudenmeir/eloquent-has-many-deep": "@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kraify\\Fastdev\\FastDevServiceProvider"
            ]
        }
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    }
}
