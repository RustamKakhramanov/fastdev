{
    "name": "kraify/fastdev",
    "description": "Traits and helpers for based functonality",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Kraify\\Fastdev\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rustam Kakhramanov"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "doctrine/dbal": "^3.8.1",
        "guzzlehttp/guzzle": "*",
        "laravel/framework": "@dev",
        "laravel/passport": "^11.10",
        "laravel/sanctum": "^3.0",
        "laravel/telescope": "^4.17",
        "spatie/laravel-permission": "^6.3",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "league/fractal": "^0.20.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kraify\\Fastdev\\FastDevServiceProvider"
            ]
        }
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    }
}
